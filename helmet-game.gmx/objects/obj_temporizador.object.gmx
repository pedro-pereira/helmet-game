        obj_centena.sprite_index = spr_tres;
        break;
    case 4:
        obj_centena.sprite_index = spr_quatro;
        break;
    case 5:
        obj_centena.sprite_index = spr_cinco;
        break;
    case 6:
        obj_centena.sprite_index = spr_seis;
        break;
    case 7:
        obj_centena.sprite_index = spr_sete;
        break;
    case 8:
        obj_centena.sprite_index = spr_oito;
        break;
    case 9:
        obj_centena.sprite_index = spr_nove;
        break;        
}

// Dezena - Dígito
switch(dezena) {
    case 0:
        obj_dezena.sprite_index = spr_zero;
        break;
    case 1:
        obj_dezena.sprite_index = spr_um;
        break;
    case 2:
        obj_dezena.sprite_index = spr_dois;
        break;
    case 3:
        obj_dezena.sprite_index = spr_tres;
        break;
    case 4:
