            <string>/// Display de pontos

// Cálculo de cada dígito do display
centena =  obj_temporizador.pontuacao div 100;
dezena  = (obj_temporizador.pontuacao mod 100) div 10;
unidade = (obj_temporizador.pontuacao mod 100) mod 10;

// Centena - Dígito
switch(centena) {
    case 0:
        obj_centena.sprite_index = spr_zero;
        break;
    case 1:
        obj_centena.sprite_index = spr_um;
        break;
    case 2:
        obj_centena.sprite_index = spr_dois;
        break;
