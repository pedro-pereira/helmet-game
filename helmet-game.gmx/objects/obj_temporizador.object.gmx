        break;
    case 5:
        obj_dezena.sprite_index = spr_cinco;
        break;
    case 6:
        obj_dezena.sprite_index = spr_seis;
        break;
    case 7:
        obj_dezena.sprite_index = spr_sete;
        break;
    case 8:
        obj_dezena.sprite_index = spr_oito;
        break;
    case 9:
        obj_dezena.sprite_index = spr_nove;
        break; 
}

// Unidade - Dígito
switch(unidade) {
    case 0:
        obj_unidade.sprite_index = spr_zero;
        break;
    case 1:
        obj_unidade.sprite_index = spr_um;
